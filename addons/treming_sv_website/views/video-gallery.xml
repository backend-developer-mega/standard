<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- === Services Page === -->
    <template name="Video Gallery" id="website.video-gallery" page="True">
        <t t-call="website.layout">

            <div class="oe_structure"/>



            <section class="open-erp-custamization odoo java text-center  ">
                <h3>
                    <span class="f-nrslight text-center grey ">
                        Treming Video  <span class="orange f-nrssemibld"> Gallery </span>
                        <div class="clear"></div>
                    </span>
                </h3>
                <div class="down-arrow"></div>

            </section>

            <section class="inner-page">
                <div class="wrapper">
                    <div class="row">
                        <div class="col8 left-section">

                            <h1 class="f-nrsthin grey">Treming Video Gallery</h1><br></br>
                            <p>Please see below  videos prepared by <span  t-field="res_company.name" itemprop="name">Company name</span></p>
                            <br></br>
                            <div class="list-works video-gallery">
                                <ul>
                                    <li>
                                        <a>
                                            <iframe width="854" height="320" src="https://www.youtube.com/embed/xPaxLLBHnoY" frameborder="0" allowfullscreen="true"></iframe>
                                            <div class="p-name"><h4>Treming - 7 years</h4></div>
                                            <span style="background-image: url(../img.youtube.com/vi/6XgKPjgyoec/maxresdefault.jpg);"><i class="fa fa-play" aria-hidden="true"></i></span>
                                        </a>
                                    </li>
                            </ul>



                            </div>
                        </div>

                        <div class="clear"></div>



                    </div>
                </div>
            </section>



            <t t-call="website.contactus_custom_template"/>
            <div class="oe_structure"/>
        </t>
    </template>

</odoo>