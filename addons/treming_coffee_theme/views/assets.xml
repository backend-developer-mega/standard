<odoo>
	<data>
	<!-- Bravo Enterprise Backend Theme Assets -->
    <template id="assets_backend" name="Enterprise Backend Assets" inherit_id="web.assets_backend">

		<xpath expr="//link[@href='/web/static/src/less/form_view_extra.less']" position="replace">
            <link rel="stylesheet" type="text/less" href="/treming_coffee_theme/static/src/less/formview.less"/>
        </xpath>

		<xpath expr="//link[@href='/web/static/src/less/form_view_layout_extra.less']" position="replace">
            <link rel="stylesheet" type="text/less" href="/treming_coffee_theme/static/src/less/formlayout.less"/>
        </xpath>

		<xpath expr=".">

			<link rel="stylesheet" href="/treming_coffee_theme/static/src/less/styles.less"/>
            <link rel="stylesheet" type="text/css" href="/treming_coffee_theme/static/lib/css/switcher.css"/>
            <link rel="stylesheet" href="/treming_coffee_theme/static/src/less/navbar.less"/>
            <link rel="stylesheet" href="/treming_coffee_theme/static/src/less/variables.less"/>
			<link rel="stylesheet" href="/treming_coffee_theme/static/src/less/app_switcher.less"/>
            <script type="application/javascript" src="/treming_coffee_theme/static/lib/js/switcher.js"/>
			<script type="application/javascript" src="/treming_coffee_theme/static/lib/js/iscroll.js"/>
            <script type="application/javascript" src="/treming_coffee_theme/static/src/js/app.js"/>
			<script type="text/javascript" src="/treming_coffee_theme/static/src/js/view.js"/>
        </xpath>
		
    </template>
	</data>
</odoo>
