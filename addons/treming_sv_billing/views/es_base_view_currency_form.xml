<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<!-- Extend form -->
		<record model="ir.ui.view" id="view_form_es_base_view_currency_inherited">
			<field name="name">es.res.currency.form</field>
			<field name="model">res.currency</field>
			<field name="inherit_id" ref="base.view_currency_form" />
			<field name="arch" type="xml">
				
				<xpath expr="//field[@name='symbol']" position="after">
					<field name="description_currency" />
				</xpath>

			</field>
		</record>
	</data>
</openerp>
